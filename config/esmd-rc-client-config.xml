<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ESMDConfig xmlns:ns2="http://esmd.ois.cms.hhs.gov/v1/rc/config">
    <ESMDSFTPServer>
        <host>eft1.feps.cms.gov</host>
        <port>9022</port>
        <environmentId>T</environmentId>
        <eftFilePrefix>#EFT</eftFilePrefix>
    </ESMDSFTPServer>
    <KeyStoreInfo>
        <keyStoreLocation>/home/ubuntu/config/rioc_gtwy_str</keyStoreLocation>
        <encKeyInfo>TYUnjVHDOBo=</encKeyInfo>
        <encKeyInfoExt>8+gsyRDQgFU=</encKeyInfoExt>
        <certAlias>riocrc</certAlias>
    </KeyStoreInfo>
    <InboundConfig>
        <enabled>true</enabled>
        <checkFrequency>1</checkFrequency>
        <rcHomeDirectory>/home/ubuntu/</rcHomeDirectory>
        <targetDirectory>/home/ubuntu/data/download</targetDirectory>
        <inputDirectory>/home/ubuntu/data/input</inputDirectory>
        <tempDirectory>/home/ubuntu/data/temp</tempDirectory>
        <errorDirectory>/home/ubuntu/data/error</errorDirectory>
        <configDirectory>/home/ubuntu/</configDirectory>
        <acknowledgmentsDirectory>/home/ubuntu/data/acknowledgment</acknowledgmentsDirectory>
        <notificationsDirectory>/home/ubuntu/data/notification</notificationsDirectory>
        <processingErrorDirectory>/home/ubuntu/data/processingError</processingErrorDirectory>
        <remoteInboundDir>/ES0047</remoteInboundDir>
        <inboundRoutingId>ES0047</inboundRoutingId>
        <ICDTDirectoryStructure>
            <inputDirectory>/home/ubuntu/data/icdt/input</inputDirectory>
            <errorDirectory>/home/ubuntu/data/icdt/error</errorDirectory>
            <notificationsDirectory>/home/ubuntu/data/icdt/ntfn_ack</notificationsDirectory>
        </ICDTDirectoryStructure>
    </InboundConfig>
    <OutboundConfig>
        <enabled>true</enabled>
        <pushFrequency>1</pushFrequency>
        <tempDirectory>/home/ubuntu/data/temp</tempDirectory>
        <outputDirectory>/home/ubuntu/data/output</outputDirectory>
        <remoteOutboundDir>/ES0047_UPLOAD</remoteOutboundDir>
        <outboundRoutingId>ESMD2</outboundRoutingId>
        <outboundFilePrefix>ON</outboundFilePrefix>
    </OutboundConfig>
</ns2:ESMDConfig>
